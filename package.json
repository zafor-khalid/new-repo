{
  "name": "za4-library",
  "version": "1.0.2",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "repository": {
    "url": "https://github.com/zafor-khalid/new-repo.git",
    "type": "git"
  },
  "license": "MIT",
  "types": "dist/index.d.ts",
  "devDependencies": {
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "concurrently": "^7.2.2",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "install-peers-cli": "^2.2.0",
    "prepend-file": "^2.0.1",
    "prettier": "^2.7.1",
    "rollup": "^2.75.7",
    "rollup-plugin-typescript2": "^0.32.1",
    "ts-node": "^10.8.2",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.2.1"
  },
  "scripts": {
    "build": "rollup -c",
    "build-watch": "rollup -c -w",
    "prepublishOnly": "npm run build",
    "postinstall": "install-peers -f && cd example && npm i",
    "start-example": "cd example && npm start",
    "deploy-example": "cd example && npm run deploy",
    "dev": "concurrently --kill-others \"npm run build-watch\" \"npm run start-example\"",
    "test": "npm run test:lint && npm run test:unit",
    "pretest": "npm run build",
    "test:unit": "ts-node test/test.ts",
    "test:lint": "eslint ./src/**/*.ts ./src/**/*.tsx ./example/src/**/*.ts ./example/src/**/*.tsx",
    "test:lint:fix": "npm run test:lint -- --fix"
  },
  "files": [
    "dist"
  ]
}
